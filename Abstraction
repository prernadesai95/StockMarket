package Abstraction;

abstract public class AbstractClass {
	
	abstract public void add();
	
	public void sub() {
		int a =10;
		int b =5;
		int c =a-b;
		System.out.println(c);

	}

abstract public void add1();
	
	public void sub1() {
		int a =11;
		int b =5;
		int c =a-b;
		System.out.println(c);
}
